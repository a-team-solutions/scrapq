{
  "name": "scrapq",
  "version": "2.0.0",
  "description": "Lightweight Typescript library for scrapping html",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "test": "npm run test:basic && npm run test:exhaustive",
    "test:basic": "jasmine-ts ./test/basic.test.ts",
    "test:exhaustive": "jasmine-ts ./test/**/*.ts",
    "dist": "npm run test; npm run build; npm run build:web",
    "build": "npm run build:node && npm run build:web && npm run build:dts",
    "build:node": "tsc",
    "build:web": "webpack --mode=production",
    "build:dts": "dts-bundle --name scrapq --main ./dist/index.d.ts"
  },
  "keywords": [
    "typescript",
    "scrapping",
    "webscrap",
    "cheerio"
  ],
  "author": {
    "name": "Daniel Derevjanik",
    "email": "daniel.derevjanik@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.8.16",
    "@types/node": "^10.17.15",
    "jasmine": "^3.5.0",
    "jasmine-ts": "^0.2.1",
    "ts-loader": "^5.4.5",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.16",
    "cheerio": "^1.0.0-rc.3"
  }
}

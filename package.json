{
  "name": "scrapq",
  "version": "1.3.4",
  "description": "Lightweight Typescript library for scrapping html",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "test": "npm run test:basic && npm run test:exhaustive",
    "test:basic": "jasmine-ts ./test/basic.test.ts",
    "test:exhaustive": "jasmine-ts ./test/**/*.ts",
    "dist": "npm run test; npm run build; npm run build:web",
    "build": "tsc && npm run build:web && npm run build:dts",
    "build:web": "webpack --mode=production",
    "build:dts": "dts-bundle --name scrapq --main ./dist/index.d.ts"
  },
  "keywords": [
    "typescript",
    "scrapping",
    "webscrap",
    "cheerio"
  ],
  "author": {
    "name": "Daniel Derevjanik",
    "email": "daniel.derevjanik@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.5.1",
    "dts-bundle": "^0.7.3",
    "jasmine": "^3.1.0",
    "jasmine-ts": "^0.2.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.0-dev.20190321",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.8",
    "cheerio": "^1.0.0-rc.2"
  }
}
